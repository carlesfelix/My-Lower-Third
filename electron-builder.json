{
  "productName": "react-electron",
  "electronDownload": {
    "cache": "./.electron-cache"
  },
  "directories": {
    "output": "release/"
  },
  "files": [
    "build/**/*",
    "node_modules/**/*"
  ],
  "win": {
    "icon": "dist",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
